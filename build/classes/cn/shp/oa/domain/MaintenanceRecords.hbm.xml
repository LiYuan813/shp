<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.shp.oa.domain">

	<class name="MaintenanceRecords" table="shp_maintenanceRecords">
		<id name="id">
            <generator class="native"/>
		</id>
		<property name="measurementRoom" />
		<property name="testingArm" />
		<property name="testingArmId" />	
		<property name="problemTime" />	
		<property name="problemDescription" />	
		<property name="solution" />	
		<property name="attachment" />
		<property name="troubleshootingTime" />
		<property name="startTime" />
		<property name="waitedTime" />
		<property name="finishTime" />
		<property name="totalTime" />
		<property name="confirmTime" />
		<property name="confirmPerson" />
		<property name="remarks" />					
	</class>
	
</hibernate-mapping>
