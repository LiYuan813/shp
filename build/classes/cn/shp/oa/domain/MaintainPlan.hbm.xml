<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.shp.oa.domain">

	<class name="MaintainPlan" table="shp_maintainPlan">
		<id name="id">
            <generator class="native"/>
		</id>
		<property name="measureroom" ></property>
 		<property name="cmmno" ></property>
		<property name="cmmtype"></property>
		<property name="plan" ></property>
		<property name="number"></property>
		<property name="xaxis"></property>
		<property name="remarks" ></property>				
	</class>
	
</hibernate-mapping>
