<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.shp.oa.domain">

	<class name="TotalInventory" table="shp_totalInventory">
		<id name="id">
            <generator class="native"/>
		</id>
		<property name="type" />	
		<property name="name" />	
		<property name="specification" />		
	    <property name="model" />	
	    <property name="brand" />	
		<property name="orderNumber" />	
		<property name="total" />	
		<property name="unit" />	
		<property name="warningQuantity" />	
		<property name="termOfValidity" />
		<property name="storageLocation" />	
		<property name="remarks" />	
		<property name="totalPrice" />	
	</class>
	
</hibernate-mapping>
