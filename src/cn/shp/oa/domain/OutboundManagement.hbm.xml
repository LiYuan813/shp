<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.shp.oa.domain">

	<class name="OutboundManagement" table="shp_outboundManagement">
		<id name="id">
            <generator class="native"/>
		</id>
		<property name="typesOfGoods" />	
		<property name="name" />	
		<property name="specification" />		
	    <property name="model" />	
	    <property name="brand" />	
		<property name="orderNumber" />	
		<property name="numberOfRecipients" />	
		<property name="unit" />	
		<property name="termOfValidity" />	
		<property name="storageLocation" />	
		<property name="useArea" />	
		<property name="person" />		
	    <property name="date" />	
	    <property name="operator" />	
		<property name="goodsId" />	
		<property name="totalPrice" />	
	</class>
	
</hibernate-mapping>
